{
  "extends": "eslint-config-airbnb",
  "rules": {
    "indent": [4, 4, {"SwitchCase": 1}],
    "no-console": [0],
    "func-names": [0],
    "semi": 2,
    "semi-spacing": 2,
    "no-else-return": 2,
    "space-infix-ops": [0],
    "react/prefer-es6-class": [0],
  },
  "globals": {
    "__PREFIX_LINKS__": true,
  },
}
